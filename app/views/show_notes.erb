<div id="all_notes">
  <% @notes.each do |note| %>
  <div id="single_note">
    <a href="/notes/<%= note.id %>">
      <h1><%= note.title %></h1>
      <p>-<%= note.signature %></p>
      <p>holes: <%= note.holes %></p>
      <p>integrity: <%= note.integrity %></p>
    </a>
  </div>
  <% end %>
</div>
